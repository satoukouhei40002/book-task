<p id="notice"><%= notice %></p>
<div class="show">
<p>
  <strong>タイトル</strong>
  <%= @read.title %>
</p>

<p>
  <strong>内容</strong>
  <%= @read.content %>
</p>

<p>
  <strong>日時</strong>
  <%= @read.start_time.strftime("%m-%d") %>
</p>

<%= link_to '編集', edit_read_path(@read) %>

<%= button_to "削除", read_path(@read), method: :delete %>

<%= link_to '戻る', reads_path %>
</div>
